{
  "manifest_version": 2,

  "name": "Froggymarks",
  "description": "Make your bookmarks useful",
  "version": "0.0.24",
  "manifest_version":2,
  "permissions": [
        "bookmarks", "storage", "background", "notifications", "tabs", "<all_urls>"
  ],

  "icons": { "16": "froggy.png",
           "48": "froggy.png",
          "128": "froggy.png" },

  "browser_action": {
    "default_icon": "froggy.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  }
}
